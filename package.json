{
  "name": "cunit-manager",
  "description": "CUnit enables simple CMake dependency management",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/cunit/cunit.git"
  },
  "scripts": {
    "dev::compile": "mkdir -p dist && npx babel src/ --out-dir dist  --source-maps inline ",
    "dev::watch": "mkdir -p dist && npx babel src/ --watch --out-dir dist  --source-maps inline"
  },
  "bin": {
    "cunit": "./bin/cunit.js"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "handlebars": "^4.0.11",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2",
    "simple-git": "^1.96.0",
    "typeguard": "0.0.12",
    "winston": "^3.0.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/handlebars": "^4.0.38",
    "@types/shelljs": "^0.8.0",
    "@types/yargs": "^11.0.0",
    "npx": "^10.2.0",
    "pkg": "^4.3.3",
    "typescript": "^2.9.2"
  }
}
